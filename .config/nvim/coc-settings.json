{
  "suggest.maxCompleteItemCount": 10,
  "suggest.floatConfig": {
    "shadow": true,
    "focusable": true
  },
  "hover.floatConfig": {
    "title": "Information",
    "border": true,
    "shadow": true,
    "focusable": true
  },
  "diagnostic.floatConfig": {
    "title": "Diagnostic",
    "shadow": true
  },
  "signature.floatConfig": {
    "title": "Signature",
    "shadow": true
  },
  "suggest.selection": "recentlyUsedByPrefix",
  "suggest.disableMenu": true,
  "suggest.disableKind": false,
  "diagnostic.messageDelay": 0,
  "diagnostic.errorSign": "",
  "diagnostic.infoSign": ">>",
  "diagnostic.warningSign": "⚠",
  "diagnostic.hintSign": ">>",
  "diagnostic.virtualText": true,
  "suggest.completionItemKindLabels": {
    "text": " Text",
    "unit": "塞Unit",
    "enum": " Enum",
    "file": " File ",
    "method": " Method",
    "function": " Function",
    "field": "ﰠ Field",
    "class": "ﴯ Class",
    "value": " Value",
    "color": " Color",
    "event": " Event",
    "module": " Module",
    "folder": " Folder",
    "struct": "פּ Struct",
    "keyword": " Keyword",
    "snippet": " Snippet",
    "variable": " Variable",
    "property": "ﰠ Property",
    "constant": " Constant",
    "operator": " Operator",
    "interface": " Interface",
    "reference": " Reference",
    "enumMember": " Enum member",
    "typeParameter": "? TypeParameter",
    "default": " Text"
