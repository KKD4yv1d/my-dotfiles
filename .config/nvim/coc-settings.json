  "diagnostic.virtualTextCurrentLineOnly": false,
  "languageserver": {
    "lua": {
      "command": "lua-language-server",
      "filetypes": ["lua"]
    },
    "kotlin": {
      "command": "~/server/bin/kotlin-language-server",
      "filetypes": ["kotlin", "kt"]
    },
    "java": {
      "command": "~/.local/lsp/java-language-server/dist/lang_server_linux.sh",
      "filetypes": ["java"]
    }
